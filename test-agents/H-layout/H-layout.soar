sp {top-state*perception-monitor*predicate-monitor*elaborate*default*predicates
   (state <s> ^superstate nil
              ^world.predicates <preds>)
-->
   (<preds> ^predicate <inp>)
   (<inp> ^handle in1)
}

sp {top-state*elaborate*agent-params*simluate-perception*true
   (state <s> ^superstate nil
              ^agent-params <p>)
   (<p> ^domain internal)
-->
   (<p> ^simulate-perception true)
}

