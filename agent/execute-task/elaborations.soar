# Elaborate child-task from superoperator.task
sp {execute-task*elaborate*child-task*superoperator
   (state <s> ^name execute-task
              ^superstate.operator.task <t>)
-->
   (<s> ^child-task <t>)
}

sp {execute-task*elaborate*task-stack*from*top-state
   (state <s> ^name execute-task
              ^top-state.task-stack <stack>)
-->
   (<s> ^task-stack <stack>)
}
              
