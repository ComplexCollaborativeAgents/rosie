sp {op_open*propose*send-open-command
   (state <s> ^name op_open1
              ^target-object <obj>
             -^current-action.open-command <cmd>)
-->
   (<s> ^operator <o> +)
   (<o> ^name send-open-command
        ^object <obj>)
}

sp {op_open*apply*send-open-command
   (state <s> ^name op_open1
              ^operator <o>
              ^top-state.io.output-link <out>
              ^current-action <a>)
   (<o> ^name send-open-command
        ^object.handle <obj-handle>)
-->
   (<out> ^set-state <set>)
   (<set> ^object-handle <obj-handle>
          ^name door1
          ^value open2)
   (<a> ^open-command <set>)
}
   
              
              
