sp {op_scan*propose*report-success*no*until
   (state <s> ^name op_scan1
              ^current-action.turn-command.status success
              -^until-clause)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {op_scan*propose*report-success*with*until
   (state <s> ^name op_scan1
              ^current-action.stop-command.status success
              ^until-clause.satisfied true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}
