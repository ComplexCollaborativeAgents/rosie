## unknown-location: no waypoint handle
sp {op_go-to1*propose*report-failure*unknown-location
   (state <s> ^name op_go-to1
              ^problem-space.action.type execute
              ^desired-location.predicates.name <loc-name>
             -^desired-waypoint)
   
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-failure
        ^failure-type unknown-location
        ^failure-info <loc-name>)
}

sp {op_go-to1*propose*report-failure*unknown-location*no*name
   (state <s> ^name op_go-to1
              ^problem-space.action.type execute
              ^desired-location.handle <loc-handle>
             -^desired-location.predicates.name <loc-name>
             -^desired-waypoint)
   
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-failure
        ^failure-type unknown-location
        ^failure-info <loc-handle>)
}
