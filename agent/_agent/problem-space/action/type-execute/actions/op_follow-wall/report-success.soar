sp {op_follow-wall*propose*report-success*finished
   (state <s> ^name op_follow-wall1
              ^current-action.follow-wall-command.status success
             -^until-clause)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {op_follow-wall*propose*report-success*stopped
   (state <s> ^name op_follow-wall1
              ^until-clause.satisfied true
              ^current-action.stop-command.status success)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}
