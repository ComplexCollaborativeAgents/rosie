# Not trying to match an action, trying to execute a composite action
sp {action*operator-tie*elaborate*state*name*selection
   (state <s> ^superstate.problem-space.name action
              ^impasse tie
              ^attribute operator
             -^superstate.match-action)
-->
   (<s> ^name selection)
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^default-state-copy no
         ^action <action>
         ^world <world>
         ^learning <learn>)
   (<action> ^type simulate)
   (<world> ^copy-type shallow)
}

sp {action*operator-tie*elaborate*state*name*action-operator-tie
  (state <s> ^superstate.problem-space.name action
             ^attribute operator
             ^impasse tie
             ^superstate.match-action <ma>)
-->
  (<s> ^name action-operator-tie)
  (<s> ^problem-space <ps>)
  (<ps> ^name action
        ^action <action>)
}

