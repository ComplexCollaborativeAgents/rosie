sp {search-for-next-action*elaborate*problem-space
   (state <s> ^name search-for-next-action)
-->
   (<s> ^default-problem-space no)

   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-space search
         ^action <a>
         ^learning <l>
         ^perception <p>
         ^world <w>)
   (<a> ^type simulate)
   (<l> ^type exploration)
   (<w> ^copy-type deep)
   (<p> ^attend-to-all-perception no)
 }

