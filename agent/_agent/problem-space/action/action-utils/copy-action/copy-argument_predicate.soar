####################################################
# predicate arguments

sp {learn-copy-action-elaboration-rule*expand*predicate*argument*copy*obj2
   (state <s> ^name learn-copy-action-elaboration-rule
              ^copy-action <ca>)
   (<ca> ^copy-argument <arg>)
   (<arg> ^arg-type predicate
          ^source-arg <src>)
   (<src> ^handle <h>
          ^2 <obj2>)
-->
   (<arg> ^obj2-copy <obj2-copy>
          ^pred-handle <h>)
   (<ca> ^copy-argument <obj2-copy>)
   (<obj2-copy> ^arg-type object
                ^source-arg.id <obj2>)
}

sp {learn-copy-action-elaboration-rule*propose*copy-argument*predicate
   (state <s> ^name learn-copy-action-elaboration-rule
              ^copy-action.copy-argument <arg>)
   (<arg> ^arg-type predicate
          ^pred-handle <h>
          ^obj2-copy.arg-copy.id <obj2-copy>
         -^arg-copy)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-argument
        ^argument <arg>
        ^arg-type predicate
        ^pred-handle <h>
        ^obj2 <obj2-copy>)
}

sp {learn-copy-action-elaboration-rule*apply*copy-argument*predicate
   (state <s> ^name learn-copy-action-elaboration-rule
              ^operator <o>)
   (<o> ^name copy-argument
        ^argument <arg>
        ^arg-type predicate
        ^pred-handle <h>
        ^obj2 <obj2>)
-->
   (<arg> ^arg-copy <copy>)
   (<copy> ^arg-type predicate
           ^handle <h>
           ^2 <obj2>)
}
