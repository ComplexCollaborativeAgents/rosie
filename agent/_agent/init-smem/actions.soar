#######################################################
### driving actions

smem --add {

# turn right|left|around
(@A1100 ^handle turn1 ^procedural <proc100> ^item-type action ^action-type primitive)
(<proc100> ^op_name op_turn1 
           ^arg1 <parg1001>)
  (<parg1001> ^arg-type concept ^importance required ^arg-status abstract ^id <conc-slot1001>)

# follow-wall right|left wall
(@A1104 ^handle follow-wall1 ^procedural <proc104> ^item-type action ^action-type primitive)
(<proc104> ^op_name op_follow-wall1 
           ^arg1 <parg1041>)
  (<parg1041> ^arg-type object ^importance required ^arg-status abstract ^id <conc-slot1041>)

# stop
(@A1106 ^handle stop1 ^procedural <proc106> ^item-type action ^action-type primitive)
(<proc106> ^op_name op_stop1)

# orient north|south|east|west
(@A1107 ^handle orient1 ^procedural <proc107> ^item-type action ^action-type primitive)
(<proc107> ^op_name op_orient1 
           ^arg1 <parg1071>)
  (<parg1071> ^arg-type concept ^importance required ^arg-status abstract ^id <conc-slot1071>)
  
# drive forward
(@A1108 ^handle drive-forward1 ^procedural <proc108> ^item-type action ^action-type primitive)
(<proc108> ^op_name op_drive-forward
           ^arg1 <parg1081>)
  (<parg1081> ^arg-type concept ^importance optional ^arg-status abstract ^id <conc-slot1081>)

# scan the room
(@A1131 ^handle scan1 ^procedural <proc31> ^item-type action ^action-type primitive ^learn-proposal true)
(<proc31> ^op_name op_scan1)

# face the object
(@A1133 ^handle face1 ^procedural <proc33> ^item-type action ^action-type primitive)
(<proc33> ^op_name op_face1
           ^arg1 <parg331>)
  (<parg331> ^arg-type object ^importance required ^arg-status real ^id <obj-slot331>)
  
}

