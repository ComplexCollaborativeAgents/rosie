sp {retrospective-learning*prefer*retrieve-initial-episode*over*copy-action
   (state <s> ^name retrospective-learning
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name retrieve-initial-episode)
   (<o2> ^name copy-action)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {retrospective-learning*prefer*copy-operator*over*subactions
   (state <s> ^name retrospective-learning
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name copy-action)
   (<o2> ^name << copy-subaction-info retrieve-subaction-episode >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {retrospective-learning*prefer*copy*subaction*over*retrieve
   (state <s> ^name retrospective-learning
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name copy-subaction-info)
   (<o2> ^name retrieve-subaction-episode)
-->
   (<s> ^operator <o1> > <o2>)
}
