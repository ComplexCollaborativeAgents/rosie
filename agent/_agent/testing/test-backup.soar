sp {topstatE*propose*learn-chunk
(state <s> ^name rosie
-^learned chunk)
-->
(<s> ^operator <o> + >)
(<o> ^name learn-chunk)
}

sp {learn-chunk*SNC*learn*on
(state <s> ^attribute state
^impasse no-change
^superstate.name learn-chunk
^superstate <ss>)
-->
(force-learn <ss>)
}

sp {learn-chunk*SNC*propose*create*result
(state <s> ^attribute state
^impasse no-change
^superstate.name learn-chunk
^superstate.superstate.name rosie)
-->
(<s> ^operator <o> + >)
(<o> ^name create-result)
}

sp {learn-chunk*SNC*apply*create-result
(state <s> ^attribute state
^impasse no-change
^superstate.name learn-chunk
^operator.name create-result
^superstate.superstate <ss>)
-->
(<ss> ^learned chunk)
}
