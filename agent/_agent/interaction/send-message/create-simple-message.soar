sp {send-message*propose*create-generic-message
   (state <s> ^name send-message
              ^message <msg>
             -^created-message)
   (<msg> ^type generic)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-generic-message)
}

sp {send-message*apply*create-generic-message
   (state <s> ^name send-message
              ^message.fields <f>
              ^operator <o>)
   (<o> ^name create-generic-message)
-->
   (<s> ^created-message <msg>)
   (<msg> ^type generic
          ^originator agent
          ^fields <f>)
}