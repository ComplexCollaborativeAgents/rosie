sp {op_put-down*goal*not*achieved*propose*do-pick-up
   (state <s> ^name op_put-down1
              ^held-object.predicates.visible true
             -^held-object.predicates.arm-status grabbed
              ^held-object <obj1>
              ^destination <obj2>
              ^predicate-handle <pred-handle>
              ^world.predicates.predicate <pred>)
   (<pred> ^handle <pred-handle>)
   -{(<pred> ^instance <i>)
     (<i> ^1 <obj1> ^2 <obj2>)}
-->
   (<s> ^operator <o> +)
   (<o> ^name do-pick-up)
}

sp {op_put-down*apply*do-pick-up*push*action
   (state <s> ^name op_put-down1
              ^operator.name do-pick-up
              ^top-state.action-stack.changes <c>
              ^held-object <obj>)
-->
   (<c> ^push <action>)
   (<action> ^action-operator <op>
             ^start-of-execution true
             ^task-subaction false)
   (<op> ^name op_pick-up1
         ^action-handle pick-up1
          ^arg1 <arg>
          ^next-action <cur>)
   (<arg> ^arg-type object
          ^id <obj>)
}

sp {op_put-down*apply*do-pick-up*remove*put-down-command
   (state <s> ^name op_put-down1
              ^operator.name do-pick-up
              ^current-action <a>)
   (<a> ^put-down-command <cmd>)
-->
   (<a> ^put-down-command <cmd> -)
}

  
