sp {evaluate-operator*monitor*go-to-next-waypoint
   (state <s> ^name evaluate-operator
              ^agent-params.debug-path-planning true
              ^operator <o>)
   (<o> ^name go-to-next-waypoint
        ^current-location.handle <cur>
        ^waypoint.handle <dest>)
-->
   (write (crlf) |Going from | <cur> | to | <dest>)
}

sp {evaluate-operator*monitor*go-to-waypoint
   (state <s> ^name evaluate-operator
              ^agent-params.debug-path-planning true
              ^operator <o>)
   (<o> ^name go-to-waypoint
        ^current-location.handle <cur>
        ^waypoint.handle <dest>)
-->
   (write (crlf) |Going from | <cur> | to | <dest>)
}

sp {evaluate-operator*monitor*go-to-adjacent-area
   (state <s> ^name evaluate-operator
              ^agent-params.debug-path-planning true
              ^operator <o>)
   (<o> ^name go-to-adjacent-area
        ^area.handle <dest>)
-->
   (write (crlf) |Trying to get to | <dest>)
}
