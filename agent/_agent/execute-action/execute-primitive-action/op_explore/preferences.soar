sp {op_explore1*prefer*attend-to-scene-change*over*others
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name attend-to-scene-change )
   (<o2> ^name << report-success report-failure mark-visited op_scan1 op_go-to-location1 >> )
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_explore1*prefer*report-success*over*others
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name report-success)
   (<o2> ^name << mark-visited op_scan1 op_go-to-location1 report-failure >> )
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_explore1*prefer*mark-visited*over*actions
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name mark-visited)
   (<o2> ^name << op_scan1 op_go-to-location1 report-failure >> )
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_explore1*prefer*scan*over*go-to-location
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +
             -^child-action)
   (<o1> ^name op_scan1)
   (<o2> ^name op_go-to-location1)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_explore1*prefer*actions*over*report-failure
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << op_scan1 op_go-to-location1 >> )
   (<o2> ^name report-failure)
-->
   (<s> ^operator <o1> > <o2>)
}


sp {op_explore1*prefer*go-to-location*shorter*distance
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name op_go-to-location1
         ^distance <d>)
   (<o2> ^name op_go-to-location1
         ^distance > <d>)
-->
   (<s> ^operator <o1> > <o2>)
}
