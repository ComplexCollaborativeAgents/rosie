# If this object doesn't match any existing ones, report this to the superstate

sp {match-existing-object*propose*matches-none
   (state <s> ^name match-existing-object)
-->
   (<s> ^operator <o> + <)
   (<o> ^name matches-none)
}

sp {match-existing-object*apply*matches-none
   (state <s> ^name match-existing-object
              ^operator.name matches-none
              ^superstate <ss>)
-->
   (<ss> ^matches-existing-object none)
}

# If print-perception is on, print a message about the match
sp {match-existing-object*apply*matches-none*print-perception
   (state <s> ^name match-existing-object
              ^operator <o>
              ^input-link-obj.object-handle <new-obj>
              ^agent-params.print-perception true)
   (<o> ^name matches-none)
-->
   (write |match-existing-object: Object | <new-obj> | doesn't match any existing objects| (crlf))
}
