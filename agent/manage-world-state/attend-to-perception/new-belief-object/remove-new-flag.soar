# If the new belief object doesn't match a exising one, remove the new flag

sp {attend-to-perception*new-belief-object*propose*remove-new-flag*matches-none
   (state <s> ^name attend-to-perception
              ^discrepancy-type new-belief-object
              ^matches-existing-object none
              ^object-info <obj-info>)
   (<obj-info> ^new-belief-object true)
-->
   (<s> ^operator <o> +)
   (<o> ^name remove-new-flag)
}

sp {attend-to-perception*new-belief-object*apply*remove-new-flag
   (state <s> ^name attend-to-perception
              ^discrepancy-type new-belief-object
              ^operator.name remove-new-flag
              ^object-info <obj-info>)
   (<obj-info> ^new-belief-object true)
-->
   (<obj-info> ^new-belief-object true -)
}

