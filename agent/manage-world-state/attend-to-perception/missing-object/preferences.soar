sp {attend-to-perception*missing-object*prefer*intersect*overlap*over*others
   (state <s> ^name attend-to-perception
              ^discrepancy-type missing-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << overlap-command intersect-command >>)
   (<o2> ^name << occlusion-command link-objects change-world-state >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-perception*missing-object*prefer*link*to*change-world-state
   (state <s> ^name attend-to-perception
              ^discrepancy-type missing-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name link-objects)
   (<o2> ^name change-world-state )
-->
   (<s> ^operator <o1> > <o2>)
}

