sp {attend-to-perception*shrunken-object*prefer*intersect*overlap*over*occlusion
   (state <s> ^name attend-to-perception
              ^discrepancy-type shrunken-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << intersect-command overlap-command >>)
   (<o2> ^name occlusion-command)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {attend-to-perception*shrunken-object*prefer*overlap-command*over*link-objects
   (state <s> ^name attend-to-perception
              ^discrepancy-type shrunken-object
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name overlap-command)
   (<o2> ^name link-objects)
-->
