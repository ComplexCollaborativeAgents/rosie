# Elaborate the source object information from the superoperator
sp {construct-world-object*elaborate*object-handle*from*superoperator
   (state <s> ^name construct-world-object
              ^superstate.operator <ssop>)
   (<ssop> ^object-handle <h>)
-->
   (<s> ^object-handle <h>)
}

# Elaborate the predicate information from the superoperator
sp {construct-world-object*elaborate*object-handle*from*superoperator
   (state <s> ^name construct-world-object
              ^superstate.operator <ssop>)
   (<ssop> ^predicates <preds>)
-->
   (<s> ^predicates <preds>)
}

# Information about where the object will be constructed
sp {construct-world-object*elaborate*destination*info*from*superoperator
   (state <s> ^name construct-world-object
              ^superstate.operator <ssop>)
   (<ssop> ^result-destination <dest>
           ^result-name <res-name>)
-->
   (<s> ^result-destination <dest>
        ^result-name <res-name>)
}

# If no destination is given, defaults to superstate
sp {construct-world-object*elaborate*destination*info*default
   (state <s> ^name construct-world-object
              ^superstate <ss>)
   (<ss> ^operator <ssop>)
   (<ssop> -^result-destination)
-->
   (<s> ^result-destination <ss>
        ^result-name constructed-world-object)
}



