# Adds the ^is-visible1 visible1 predicate
sp {anystate*apply*attend-to-scene-change*attend-to-different-reachable*add*new
   (state <s> ^operator <o>)
   (<o> ^name attend-to-scene-change
        ^change <c>)
   (<c> ^type different-reachable-predicate
        ^object-info.wm-obj.predicates <wm-preds>
        ^is-reachable1 <new>)
-->
   (<wm-preds> ^is-reachable1 <new>)
}

# Removes the ^is-visible1 not-visible1 predicate
sp {anystate*apply*attend-to-scene-change*attend-to-different-reachable*remove*old
   (state <s> ^operator <o>)
   (<o> ^name attend-to-scene-change
        ^change <c>)
   (<c> ^type different-reachable-predicate
        ^object-info.wm-obj.predicates <wm-preds>
        ^is-reachable1 <new>)
   (<wm-preds> ^is-reachable1 { <old> <> <new> })
-->
   (<wm-preds> ^is-reachable1 <old> -)
}

