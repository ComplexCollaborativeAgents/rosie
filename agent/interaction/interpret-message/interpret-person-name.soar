
sp {interpret-message*propose*interpret-person-name
   (state <s> ^name interpret-message
              ^instructor-message <msg>)
   (<msg> ^message-type williams
         -^interpreted true)
-->
   (<s> ^operator <o> +)
   (<o> ^name interpret-person-name
        ^message <msg>
        ^person-name williams1)
}

sp {interpret-message*apply*interpret-person-name*answer*to*ask*add*answer*predicate
   (state <s> ^name interpret-message
              ^operator <o>
              ^instructor-message <msg>
              ^top-state.interaction.stack.segment.purpose.type wait-for-answer
              ^top-state.task-stack.top <top>)
   (<o> ^name interpret-person-name
        ^person-name williams1)
   (<top> ^task-operator.task-handle ask1)
-->
   (<top> ^answer <obj>)
   (<obj> ^item-type object
          ^handle person-williams1
          ^predicates <preds>
          ^root-category officer1)
   (<preds> ^category entity1 person officer1
            ^name williams1)
   (<s> ^mark-interpreted <msg>)
}
