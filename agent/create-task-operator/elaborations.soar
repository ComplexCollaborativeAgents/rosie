sp {create-task-operator*elaborate*world*from*superstate
  (state <s> ^name create-task-operator
             ^superstate.world <world>)
-->
  (<s> ^world <world>)
}

# command-message: the parsed command representation from language comprehension
sp {create-task-operator*elaborate*message*from*superoperator
   (state <s> ^name create-task-operator
              ^superstate.operator.command-message <msg>)
-->
   (<s> ^command-message <msg>)
}

# argument-to-create: structure created for each argument on the command-message
sp {create-task-operator*elaborate*argument-to-create
   (state <s> ^name create-task-operator
              ^command-message.<arg-name> <arg>)
   (<arg> ^item-type <arg-type>)
-->
   (<s> ^argument-to-create <atc>)
   (<atc> ^arg-name <arg-name>
          ^arg-type <arg-type>
          ^message-arg <arg>)
}

