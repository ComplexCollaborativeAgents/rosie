sp {anystate*copy-action*reject*all*operators*force*SNC
   (state <s> -^name learn-copy-action-elaboration-rule
              ^copy-action <ca>
              ^operator <o> +)
   (<ca> ^destination <dest>
         ^copy-name <name>)
   (<dest> -^<name>)
-->
   (<s> ^operator <o> -
        ^learning-chunking-rule true)
}

sp {anystate*copy-action*elaborate*copy-action-present
   (state <s> -^name learn-copy-action-elaboration-rule
              ^copy-action <ca>)
-->
   (<s> ^copy-action-present true)
}
              
