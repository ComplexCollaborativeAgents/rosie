sp {anystate*propose*ground-action
   (state <s> ^ground-action <ga>
              ^world <w>)
   (<ga> ^source-id <src-id>
         ^reference-action <ref-action>
         ^reference-id <ref-id>
        -^grounded-action)
-->
   (<s> ^operator <o> + =)
   (<o> ^name ground-action
        ^info <ga>
        ^source-id <src-id>
        ^reference-action <ref-action>
        ^reference-id <ref-id>
        ^world <w>)
}
