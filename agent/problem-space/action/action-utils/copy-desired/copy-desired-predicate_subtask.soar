sp {task-utils*copy-desired*propose*copy-desired-predicate*subtask
   (state <s> ^copy-desired-workspace <ws>)
   (<ws> ^copy-predicate <pred>)
   (<pred> ^source-pred <src-pred>
          -^pred-copy)
   (<src-pred> ^type subtask
               ^subtask-handle <sub-handle>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-desired-predicate
        ^workspace <ws>
        ^predicate <pred>
        ^type subtask
        ^subtask-handle <sub-handle>)
}

sp {task-utils*copy-desired*apply*copy-desired-predicate*subtask
   (state <s> ^operator <o>)
   (<o> ^name copy-desired-predicate
        ^workspace <ws>
        ^predicate <pred>
        ^type subtask
        ^subtask-handle <sub-handle>)
-->
   (<pred> ^pred-copy <copy>)
   (<copy> ^item-type goal-predicate
           ^type subtask
           ^subtask-handle <sub-handle>)
}
