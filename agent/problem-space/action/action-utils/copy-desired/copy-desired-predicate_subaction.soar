sp {action-utils*copy-desired*propose*copy-desired-predicate*subaction
   (state <s> ^copy-desired-workspace <ws>)
   (<ws> ^copy-predicate <pred>)
   (<pred> ^source-pred <src-pred>
          -^pred-copy)
   (<src-pred> ^type subaction
               ^subaction-handle <sub-handle>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-desired-predicate
        ^workspace <ws>
        ^predicate <pred>
        ^type subaction
        ^subaction-handle <sub-handle>)
}

sp {action-utils*copy-desired*apply*copy-desired-predicate*subaction
   (state <s> ^operator <o>)
   (<o> ^name copy-desired-predicate
        ^workspace <ws>
        ^predicate <pred>
        ^type subaction
        ^subaction-handle <sub-handle>)
-->
   (<pred> ^pred-copy <copy>)
   (<copy> ^item-type goal-predicate
           ^type subaction
           ^subaction-handle <sub-handle>)
}
