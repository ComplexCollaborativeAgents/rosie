   (state <s> ^operator <o>)
   (<o> ^name copy-desired-subgoal
        ^subgoal <sub>)
   (<sub> ^pred-count 0)
-->
   (<sub> ^subgoal-copy <copy>)
   (<copy> ^item-type task-subgoal 
           ^pred-count 0)
}

