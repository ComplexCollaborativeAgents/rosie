sp {copy-desired*propose*copy-desired-subgoal
   (state <s> ^copy-desired-workspace <ws>)
   (<ws> ^copy-subgoal <sub>)
   (<sub> -^subgoal-copy <copy>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-desired-subgoal
        ^workspace <ws>
        ^subgoal <sub>)
}

# ENUMERATE: copy subgoals with 0-5 predicates

