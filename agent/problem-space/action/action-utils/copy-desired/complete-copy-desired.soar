sp {action-utils*copy-desired*propose*complete-copy-desired
   (state <s> ^copy-desired-workspace <ws>)
-->
   (<s> ^operator <o> + < =)
   (<o> ^name complete-copy-desired
        ^workspace <ws>)
}

# Creates the deep copy of the desired structure on the superstate
# (Will create the chunked application rule)
# ENUMERATE: 1-5 subgoals

sp {copy-desired*complete-copy-desired*learn-copy-desired-elaboration-rule*1*subgoals
   (state <s> ^operator <o>
              ^superstate <ss>)
   (<o> ^name complete-copy-desired
        ^workspace <ws>)
   (<ws> ^source-desired <src>
         ^subgoal-count 1
         ^copy-subgoal <sub1>)
   (<src> ^handle <h>)
   (<sub1> ^num 1 ^subgoal-copy <copy1>)
-->
   (<ss> ^desired <des>)
   (<des> ^duplicate-of <src>
            ^item-type task-goal
            ^subgoal-count 1
            ^handle <h>
            ^1 <copy1>)
}

