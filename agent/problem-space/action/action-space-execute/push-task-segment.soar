sp {action-space*execute*propose*push-task-segment*new-task-segment*on*stack
   (state <s> ^problem-space.action-space execute
              ^task-stack.new-task-segment <seg>
             -^child-task-seg)
-->
   (<s> ^operator <o> +)
   (<o> ^name push-task-segment
        ^segment <seg>)
}

sp {action-space*execute*propose*push-task-segment*new-task-segment*on*state
   (state <s> ^problem-space.action-space execute
              ^new-task-segment <seg>)
-->
   (<s> ^operator <o> +)
   (<o> ^name push-task-segment
        ^segment <seg>)
}

