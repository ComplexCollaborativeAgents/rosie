sp {action-space*execute*propose*add-subtask-to-tcn*action-source*instruction
   (state <s> ^problem-space.action-space execute
              ^child-action <a>
              ^task-operator <action>)
   (<a> ^action-source instruction
        ^task-operator <subtask>)
-->
   (<s> ^operator <o> +)
   (<o> ^name add-subtask-to-tcn
        ^subtask <subtask>
        ^task-operator <action>)
}

sp {action-space*execute*apply*add-subtask-to-tcn*finished*remove*action-soruce
   (state <s> ^problem-space.action-space execute
              ^operator.name add-subtask-to-tcn
              ^child-action <a>
              ^current-action <ca>
              ^created-subtask <sub-h>)
   (<a> ^action-source instruction)
-->
   (<a> ^action-source instruction -)
   (<s> ^created-subtask <sub-h> -)
   (<ca> ^learn-subtask-proposal <sub-h>)
}
    
sp {action-space*execute*apply*add-subtask-to-tcn*finished*add*subtask-handle
   (state <s> ^problem-space.action-space execute
              ^operator.name add-subtask-to-tcn
              ^child-action <a>
              ^created-subtask <sub-h>)
   (<a> ^action-source instruction
        ^task-operator <ex-op>)
-->
   (<ex-op> ^subtask-handle <sub-h>)
   (<a> ^assigned-subtask-handle true)
}
    
