              ^action-to-push.execution-operator <a>
             -^predicate-lti.handle <pred-handle>)
   (<a> ^<arg-name> <arg>)
   (<arg> ^arg-type predicate
          ^handle <pred-handle>)
-->
   (<s> ^smem-query <q>)
   (<q> ^cue.handle <pred-handle>
        ^depth 4
        ^destination <s>
        ^result-name predicate-lti)
}

