sp {action-space*execute*action*status*success*mark*performed*subaction
   (state <s> ^problem-space.action-space execute
              ^operator <o>
              ^action-operator <aop>
              ^child-action <child>
              ^action-stack <stack>)
   (<child> ^execution-operator <o>
            ^status success)
   (<o> ^perform once
        ^subaction-handle { <sub-h> <> none })
-->
   (<aop> ^performed <sub-h>)
}

sp {action-space*execute*action*status*success*mark*performed-subaction
   (state <s> ^problem-space.action-space execute
              ^operator <o>
              ^action-operator <aop>
              ^child-action <child>
              ^action-stack <stack>)
   (<child> ^execution-operator <o>
            ^status success)
   (<o> ^subaction-handle { <sub-h> <> none })
-->
   (<aop> ^performed-subaction <sub-h>)
}
