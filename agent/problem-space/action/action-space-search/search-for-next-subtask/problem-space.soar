sp {search-for-next-subtask*elaborate*problem-space
   (state <s> ^name search-for-next-subtask)
-->
   (<s> ^default-problem-space no)

   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-space search
         ^action <a>
         ^learning <l>
         ^perception <p>
         ^world <w>)
   (<l> ^type exploration)
   (<w> ^copy-type deep)
   (<p> ^attend-to-all-perception no)
}

