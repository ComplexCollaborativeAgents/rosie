sp {search-for-next-subtask*top-search-state*propose*return-created-subtask
   (state <s> ^problem-space.action-space search
              ^top-search-state true
              ^created-subtask <sub-h>
              ^successful-operator <sub-op>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name return-created-subtask
        ^created-subtask <sub-h>)
}

sp {search-for-next-subtask*top-search-state*apply*return-created-subtask*copy*created-subtask*superstate
   (state <s> ^problem-space.action-space search
              ^top-search-state true
              ^operator <o>
              ^superstate <ss>)
   (<o> ^name return-created-subtask
        ^created-subtask <sub-h>)
-->
   (<ss> ^created-subtask <sub-h>)
}
