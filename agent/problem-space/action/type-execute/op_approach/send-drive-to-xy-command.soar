sp {op_approach1*propose*send-drive-to-xy-command
   (state <s> ^name op_approach1
              ^problem-space.action.type execute
              ^target-object <object>
              ^within-range false
              ^calculations.drive-x <x>
              ^calculations.drive-y <y>
              ^performed-command.name orient
              -^current-action.sent-command.name drive-xy)
-->
   (<s> ^operator <o> + )
   (<o> ^name send-drive-to-xy-command
        ^position <pos>)
   (<pos> ^x <x> ^y <y>)
}

sp {op_approach1*apply*send-drive-to-xy-command*send
   (state <s> ^name op_approach1
              ^operator <o>
              ^io.output-link <out>
              ^current-action <a>)
   (<o> ^name send-drive-to-xy-command
        ^position <pos>)
   (<pos> ^x <x> ^y <y>)
-->
    (<out> ^do-control-law <law>)
    (<law> ^name drive-xy
           ^parameters <params>
           ^termination-condition.name stabilized)
    (<params> ^x <x> ^y <y>)
    (<a> ^sent-command <law>)
}
