sp {op_start-leading1*propose*report-success
  (state <s> ^name op_start-leading1
             ^current-action.received-yes)
-->
  (<s> ^operator <o> + >)
  (<o> ^name report-success)
}

sp {op_start-leading1*apply*report-success*mark*leading
  (state <s> ^name op_start-leading1
             ^operator.name report-success
             ^person.predicates <preds>)
-->
  (<preds> ^robot-status leading)
}
