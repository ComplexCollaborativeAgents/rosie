sp {op_put-down1*elaborate*held-object*task-operator
   (state <s> ^name op_put-down1
              ^task-operator.arg1.id <obj>)
-->
   (<s> ^held-object <obj>)
}

sp {op_put-down1*elaborate*predicate-handle*task-operator
   (state <s> ^name op_put-down1
              ^task-operator.arg2.handle <pred-handle>)
-->
   (<s> ^predicate-handle <pred-handle>)
}

sp {op_put-down1*elaborate*destination*task-operator
   (state <s> ^name op_put-down1
              ^task-operator.arg2.{2} <obj>)
-->
   (<s> ^destination <obj>)
}

# If no arg2, the goal is the object is not grabbed
sp {op_put-down1*elaborate*desired*no*arg2
   (state <s> ^name op_put-down1
             -^copy-task-goal
              ^task-operator <task-op>
              ^held-object <obj>)
   (<task-op> ^current-goal put-down1goal1
             -^arg2)
-->
   (<s> ^desired <des>)
   (<des> ^handle put-down1goal1 ^pred-count 1 ^1 <p1>)
   (<p1> ^type unary ^handle not-grabbed1 ^property-handle is-grabbed1 ^1 <obj>)
}

