sp {op_remember1*propose*change-world-state*merge-belief-objects
   (state <s> ^name op_remember1
              ^problem-space.execution-type external
              ^task-operator.current-goal remember1goal1
              ^top-state.perception-monitor.object-monitor <obj-mon>
              ^object1 <obj1>
              ^object2 <obj2>)
   (<obj-mon> ^object-info <obj1-info> { <obj2-info> <> <obj1-info> })
   (<obj1-info> ^world-obj <obj1>)
   (<obj2-info> ^world-obj <obj2>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name change-world-state
        ^merge-belief-objects <merge>)
   (<merge> ^core-object-info <obj1-info>
            ^merging-object-info <obj2-info>)
}

sp {op_remember1*apply*change-world-state*merge-belief-objects*mark*merged
   (state <s> ^name op_remember1
              ^operator <o>)
   (<o> ^name change-world-state
        ^merge-belief-objects <merge>)
-->
   (<s> ^merged-objects true)
}

sp {op_remember1*apply*change-world-state*merge-belief-objects*mark*object1*task-object*parent
   (state <s> ^name op_remember1
              ^operator <o>
              ^object1 <obj1>
              ^parent-task-segment.task-objects <objs>)
   (<o> ^name change-world-state
        ^merge-belief-objects <merge>)
-->
   (<objs> ^added-object <obj1>)
}

