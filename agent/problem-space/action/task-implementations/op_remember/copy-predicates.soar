sp {op_remember1*propose*copy-predicates
   (state <s> ^name op_remember1
              ^problem-space.execution-type external
              ^uncopied-predicate <uc>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-predicates)
}

sp {op_remember1*apply*copy-predicates
   (state <s> ^name op_remember1
              ^operator.name copy-predicates
              ^uncopied-predicate.<prop> <pred>
              ^object1.predicates <o1preds>)
-->
   (<o1preds> ^<prop> <pred>)
}

sp {op_remember1*apply*copy-predicates*2
   (state <s> ^name op_remember1
              ^operator.name copy-predicates
              ^uncopied-predicate.<prop> <pred>
              ^object2.predicates <o2preds>)
-->
   (<o2preds> ^<prop> <pred>)
}
