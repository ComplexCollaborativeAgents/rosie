sp {op_remember1*elaborate*objects*superoperator
   (state <s> ^name op_remember1
              ^task-operator <ssop>)
   (<ssop> ^arg1.id <obj1>
           ^arg2.id <obj2>)
-->
   (<s> ^object1 <obj1>
        ^object2 <obj2>)
}

sp {op_remember1*elaborate*uncopied-predicate
   (state <s> ^name op_remember1
              ^object1.predicates <o1preds>
              ^object2.predicates <o2preds>)
   (<o2preds> ^{ <prop> << modifier1 category >> } <pred>)
   (<o1preds> -^<prop> <pred>)
-->
   (<s> ^uncopied-predicate.<prop> <pred>)
}

#### GOAL SATISFACTION ####

sp {op_remember1*elaborate*desired
   (state <s> ^name op_remember1
              ^task-operator.current-goal remember1goal1
             -^copy-task-goal)
-->
   (<s> ^desired <des>)
   (<des> ^handle remember1goal1 ^pred-count 0)
}

sp {op_remember2*elaborate*desired*satisfied
   (state <s> ^name op_remember1
              ^desired <des>
             -^uncopied-predicate)
   (<des> ^handle remember1goal1)
-->
   (<des> ^satisfied true)
}
