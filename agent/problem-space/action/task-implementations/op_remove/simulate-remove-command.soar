sp {op_remove1*propose*simulate-remove-command
   (state <s> ^name op_remove1
              ^problem-space.execution-type external
              ^current-task-segment <seg>)
   (<seg> ^remove-command <cmd>)
   (<cmd> ^name remove
         -^status complete)
-->
   (<s> ^operator <o> +)
   (<o> ^name simulate-remove-command
        ^item-type command-operator
        ^command <cmd>)
}

sp {op_remove1*apply*simulate-remove-command*mark*command*complete
   (state <s> ^name op_remove1
              ^operator <o>)
   (<o> ^name simulate-remove-command
        ^command <cmd>)
-->
   (<cmd> ^status complete)
}

sp {op_remove1*apply*simulate-remove-command*add*value
   (state <s> ^name op_remove1
              ^operator.name simulate-remove-command
              ^target-object.predicates <preds>
              ^target-value <val>)
-->
   (<preds> ^value <val>)
}

sp {op_remove1*apply*simulate-remove-command*add*world-vis*output-link
   (state <s> ^name op_remove1
              ^operator.name simulate-remove-command
              ^target-value <val>
              ^target-object.predicates.value <val>
              ^top-state.world.objects <objs>
              ^top-state.io.output-link <ol>)
-->
   (<ol> ^world-vis <wv>)
   (<wv> ^objects <objs>)
}

