
# Reject a label operator where both objects are the same
sp {anystate*reject*op_label1*same*object
   (state <s> ^operator <o> +)
   (<o> ^name op_label1
        ^arg1.id <obj>
        ^arg2.id <obj>)
-->
   (<s> ^operator <o> -)
}

# Reject a label operator if both objects are confirmed
sp {anystate*reject*op_label1*both*objects*confirmed
   (state <s> ^operator <o> +)
   (<o> ^name op_label1
        ^arg1.id.predicates.is-confirmed1 confirmed1
        ^arg2.id.predicates.is-confirmed1 confirmed1)
-->
   (<s> ^operator <o> -)
}
