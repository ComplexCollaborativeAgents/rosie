sp {problem-space*action*task-implementations*prefer*others*over*find
   (state <s> ^problem-space.name action
              ^problem-space.subproblem execute
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^item-type task-operator
         ^name <> op_find1
         ^<arg-name>.<att> <obj>)
   (<o2> ^name op_find1
         ^arg1.id <obj>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {problem-space*action*reject*task-operator*if*perform*once
   (state <s> ^problem-space.name action
              ^operator <o> +
              ^task-operator.performed-subtask <sub-h>)
   (<o> ^item-type task-operator
        ^subtask-handle <sub-h>
        ^perform once)
-->
   (<s> ^operator <o> -)
}

