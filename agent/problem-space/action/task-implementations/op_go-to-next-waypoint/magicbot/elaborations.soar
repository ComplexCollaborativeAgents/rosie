
#### GOAL SATISFACTION ####
sp {op_go-to-next-waypoint1*elaborate*desired
   (state <s> ^name op_go-to-next-waypoint1
              ^problem-space.execution-type external
              ^task-operator.current-goal go-to-next-waypoint1goal1
             -^copy-task-goal)

-->
   (<s> ^desired <des>)
   (<des> ^handle go-to-next-waypoint1goal1 ^pred-count 0)
}

sp {op_go-to-next-waypoint1*elaborate*desired*satisfied
   (state <s> ^name op_go-to-next-waypoint1
              ^problem-space.execution-type external
              ^desired <des>
              ^current-task-segment.drive-command.status success
              ^current-task-segment.position-set.next.next none)
   (<des> ^handle go-to-next-waypoint1goal1)
-->
   (<des> ^satisfied true)
}


sp {op_go-to-next-waypoint1*elaborate*waypoint
   (state <s> ^name op_go-to-next-waypoint1
              ^problem-space.execution-type external
              ^task-operator.arg1 <arg1>)
   (<arg1> ^arg-type waypoint
           ^id <wp>)
-->
   (<s> ^waypoint <wp>)
}

# Note: in the real world this should always be true,
# During development we can save time by removing this
sp {op_go-to-next-waypoint1*elaobrate*do-face
  (state <s> ^name op_go-to-next-waypoint1
             ^problem-space.execution-type external)
-->
  (<s> ^do-face true)
}

# Note: in the real world this should always be false
# During development we can save time by removing this
sp {op_go-to-next-waypoint1*elaobrate*ignore-doors
  (state <s> ^name op_go-to-next-waypoint1
             ^problem-space.execution-type external)
-->
  (<s> ^ignore-doors false)
}


