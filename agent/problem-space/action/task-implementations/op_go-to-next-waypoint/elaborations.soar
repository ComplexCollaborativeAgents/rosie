              ^problem-space.execution-type external
              ^task-operator.arg1 <arg1>)
   (<arg1> ^arg-type waypoint
           ^id <wp>)
-->
   (<s> ^waypoint <wp>)
}

sp {problem-space*action*internal*elaborate*op_go-to-next-waypoint1*dont-learn-action-model
   (state <s> ^problem-space.execution-type internal
              ^operator <o> +)
   (<o> ^name op_go-to-next-waypoint1)
-->
   (<o> ^dont-learn-action-model true)
}

sp {op_go-to-next-waypoint1*elaborate*terminal*superoperator
   (state <s> ^name op_go-to-next-waypoint1
              ^superstate.operator.terminal <flag>)
-->
   (<s> ^terminal <flag>)
}
