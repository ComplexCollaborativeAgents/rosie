sp {op_go-to-next-waypoint1*elaborate*waypoint
   (state <s> ^name op_go-to-next-waypoint1
              ^problem-space.execution-type external
              ^task-operator.arg1 <arg1>)
   (<arg1> ^arg-type waypoint
           ^id <wp>)
-->
   (<s> ^waypoint <wp>)
}

#### CUSTOM GOAL HANDLING ####

sp {op_go-to-next-waypoint1*elaborate*desired
   (state <s> ^name op_go-to-next-waypoint1
              ^problem-space.execution-type external
              ^task-operator.current-goal go-to-next-waypoint1goal1
             -^copy-task-goal
              ^waypoint <wp>)
-->
   (<s> ^desired <des>)
   (<des> ^handle go-to-next-waypoint1goal1 ^pred-count 1 ^1 <p1>)
   (<p1> ^type status ^name current-waypoint ^1 <wp>)
}

