# Repeat isn't really a real task, so immediately finished

sp {op_repeat*elaborate*desired
   (state <s> ^name op_repeat1
              ^task-operator.current-goal repeat1goal1
             -^copy-task-goal)
-->
   (<s> ^desired <des>)
   (<des> ^handle repeat1goal1 ^pred-count 0)
}

sp {op_repeat*elaborate*desired*satisfied
   (state <s> ^name op_repeat1
              ^desired <des>)
   (<des> ^handle repeat1goal1)
   -->
   (<des> ^satisfied true)
}
