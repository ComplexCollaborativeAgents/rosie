sp {op_find1*propose*remove-recalled-object
   (state <s> ^name op_find1
              ^world.objects.object <obj>)
   (<obj> ^predicates <preds>)
   (<preds> ^modifier1 recalled1
            ^category nothing1)
-->
   (<s> ^operator <o> +)
   (<o> ^name remove-recalled-object
        ^object <obj>)
}

sp {op_find1*apply*remove-recalled-object
   (state <s> ^name op_find1
              ^operator <o>
              ^world.objects <objs>)
   (<o> ^name remove-recalled-object
        ^object <obj>)
   (<objs> ^object <obj>)
-->
   (<objs> ^object <obj> -)
}
