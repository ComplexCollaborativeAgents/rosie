
sp {subproblem*execute*propose*init-current-goal
   (state <s> ^problem-space.subproblem execute
              ^task-operator <task-op>
              ^task-concept-network.goal-graph <start>)
   (<task-op> -^current-goal)
   (<start> ^handle <goal-handle>
            ^item-type start-goal)
-->
   (<s> ^operator <o> + =)
   (<o> ^name init-current-goal
        ^goal-handle <goal-handle>)
}

sp {subproblem*execute*apply*init-current-goal
   (state <s> ^problem-space.subproblem execute
              ^operator <o>
              ^task-operator <task-op>)
   (<o> ^name init-current-goal
        ^goal-handle <goal-handle>)
-->
   (<task-op> ^current-goal <goal-handle>)
}


