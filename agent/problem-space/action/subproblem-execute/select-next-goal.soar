sp {subproblem*execute*apply*select-next-goal*update*current-goal
   (state <s> ^problem-space.subproblem execute
              ^operator <o>
              ^task-operator <task-op>)
   (<o> ^name select-next-goal
        ^next-goal-handle <next-goal-h>)
   (<task-op> ^current-goal { <goal-h> <> <next-goal-h> })
-->
   (<task-op> ^current-goal <goal-h> -
              ^current-goal <next-goal-h>)
}
