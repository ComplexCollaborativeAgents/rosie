sp {subproblem*execute*propose*push-task-segment*new-task-segment*on*state
   (state <s> ^problem-space.subproblem execute
              ^new-task-segment <seg>)
-->
   (<s> ^operator <o> +)
   (<o> ^name push-task-segment
        ^segment <seg>)
}

sp {subproblem*execute*apply*push-task-segment*complete*remove*new-task-segment
   (state <s> ^problem-space.subproblem execute
              ^operator <o>
              ^new-task-segment <seg>)
   (<o> ^name push-task-segment
        ^segment <seg>
        ^status complete)
-->
   (<s> ^new-task-segment <seg> -)
}

