sp {learn-new-subtask*prefer*add-subtask-to-tcn*over*others
   (state <s> ^name learn-new-subtask
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name add-subtask-to-tcn)
   (<o2> ^name << add-node-to-goal-graph add-composite-goal-predicate store-subtask >>)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {learn-new-subtask*prefer*add-node-to-goal-graph*over*store-subtask
   (state <s> ^name learn-new-subtask
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << add-node-to-goal-graph add-composite-goal-predicate >>)
   (<o2> ^name store-subtask)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {learn-new-subtask*prefer*store-subtask*over*complete
   (state <s> ^name learn-new-subtask
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name store-subtask)
   (<o2> ^name complete-learn-new-subtask)
-->
   (<s> ^operator <o1> > <o2>)
}
