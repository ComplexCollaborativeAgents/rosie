sp {search-for-next-subtask*propose*increment-search-level
   (state <s> ^name search-for-next-subtask
              ^subtask-search-failed <lvl>
              ^problem-space.search-level { <lvl> < 3 })
-->
   (<s> ^operator <o> + >)
   (<o> ^name increment-search-level
        ^current-level <lvl>)
}

sp {search-for-next-subtask*apply*increment-search-level
   (state <s> ^name search-for-next-subtask
              ^operator <o>
              ^problem-space <ps>
              ^subtask-search-failed <lvl>)
   (<o> ^name increment-search-level
        ^current-level <lvl>)
   (<ps> ^search-level <lvl>)
-->
   (<s> ^subtask-search-failed <lvl> -)
   (<ps> ^search-level <lvl> -
         ^search-level (+ <lvl> 1))
}
