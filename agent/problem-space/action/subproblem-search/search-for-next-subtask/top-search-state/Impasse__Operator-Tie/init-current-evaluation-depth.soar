sp {subproblem*search*selection*apply*init-current-evaluation-depth*level*1
   (state <s> ^name selection
              ^problem-space <ps>
              ^top-selection true
              ^superstate.desired.pred-count <any>
              ^operator <o>)
   (<ps> ^subproblem search
         ^search-level 1)
   (<o> ^name init-current-evaluation-depth)
-->
   # Change to this set the iterative-deepening max search depth
   (<s> ^max-evaluation-depth 4)
}

sp {subproblem*search*selection*apply*init-current-evaluation-depth*level*2
   (state <s> ^name selection
              ^problem-space <ps>
              ^top-selection true
              ^superstate.desired.pred-count <any>
              ^operator <o>)
   (<ps> ^subproblem search
         ^search-level 2)
   (<o> ^name init-current-evaluation-depth)
-->
   # Change to this set the iterative-deepening max search depth
   (<s> ^max-evaluation-depth 2)
}

sp {subproblem*search*selection*apply*init-current-evaluation-depth*level*3
   (state <s> ^name selection
              ^problem-space <ps>
              ^top-selection true
              ^superstate.desired.pred-count <any>
              ^operator <o>)
   (<ps> ^subproblem search
         ^search-level 3)
