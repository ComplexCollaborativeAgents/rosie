sp {search-for-next-subtask*top-search-state*propose*return-search-failure
   (state <s> ^problem-space.subproblem search
              ^top-search-state true
              ^subtask-search-failed true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name return-search-failure)
}

sp {search-for-next-subtask*top-search-state*apply*return-search-failure*report*subtask-search-failed*superstate
   (state <s> ^problem-space.subproblem search
              ^top-search-state true
              ^operator.name return-search-failure
              ^superstate <ss>)
-->
   (<ss> ^subtask-search-failed true)
}
