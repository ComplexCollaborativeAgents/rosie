   (<ps> ^name action
         ^subproblem search
         ^propose-subtasks true)
   (<info> ^subtask-op.<arg-name> <arg>)
   (<arg> ^arg-type predicate
          ^2 <obj>)
   (<objs> -^object <obj>)
