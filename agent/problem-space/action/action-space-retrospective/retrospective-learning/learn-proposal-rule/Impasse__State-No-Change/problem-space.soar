


sp {learn-proposal-rule*elaborate*problem-space
   (state <s> ^name learn-proposal-rule)
-->
   (<s> ^default-problem-space no)
   
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^execution-type internal
         ^world <world>
         ^learning <learn>)
   (<learn> ^type retrospective
            ^use chunking)
}

sp {learn-proposal-rule*interrupt
   (state <s> ^name learn-proposal-rule)
-->
   (interrupt)
}

