sp {replay-subaction*propose*return-learning-result
   (state <s> ^name replay-subaction
              ^retrospective-learning-result <res>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name return-learning-result
        ^result <res>)
}

sp {replay-subaction*apply*return-learning-result*copy*to*superstate
   (state <s> ^name replay-subaction
              ^operator <o>
              ^subaction-context <sub>)
   (<o> ^name return-learning-result
        ^result <res>)
-->
   (<sub> ^learning-result <res>)
}
