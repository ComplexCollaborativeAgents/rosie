sp {action-space*retrospective*selection*elaborate*match-task-operator
   (state <s> ^name selection
              ^problem-space.action-space retrospective
              ^subtasks.next { <sub> <> none })
-->
   (<s> ^match-task-operator <mto>)
   (<mto> ^task-operator <sub>
          ^candidate-tasks <cands>)
}

sp {action-space*retrospective*selection*elaborate*match-task-operator*candidate-task-operator
   (state <s> ^name selection
              ^problem-space.action-space retrospective
              ^item <o>
              ^match-task-operator.candidate-tasks <cands>)
-->
   (<cands> ^candidate-task-operator <o>)
}
