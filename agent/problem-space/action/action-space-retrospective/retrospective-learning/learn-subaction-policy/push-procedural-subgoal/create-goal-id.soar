sp {push-procedural-subgoal*propose*create-goal-id
   (state <s> ^name push-procedural-subgoal
              ^action-id <aid>)
   (<aid> -^goal)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-goal-id)
}

sp {push-procedural-subgoal*apply*create-goal-id
   (state <s> ^name push-procedural-subgoal
              ^operator.name create-goal-id
              ^action-id <aid>)
-->
   (<aid> ^goal <g>)
   (<g> ^handle (make-constant-symbol |taskgoal|)
        ^item-type task-goal
        ^subgoal-count 0)
}
