sp {learn-subaction-policy*propose*return-learning-result*policy*success
   (state <s> ^name learn-subaction-policy
              ^retrospective-learning-result success)
-->
   (<s> ^operator <o> + >)
   (<o> ^name return-learning-result
        ^result success)
}

sp {learn-subaction-policy*propose*return-learning-result*procedural*success
   (state <s> ^name learn-subaction-policy
              ^procedural-learning-result success)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name return-learning-result
        ^result success)
}

sp {learn-subaction-policy*apply*return-learning-result*copy*to*superstate
   (state <s> ^name learn-subaction-policy
              ^operator <o>
              ^subaction-context <sub>)
   (<o> ^name return-learning-result
        ^result <res>)
-->
   (<sub> ^learning-result <res>)
}
