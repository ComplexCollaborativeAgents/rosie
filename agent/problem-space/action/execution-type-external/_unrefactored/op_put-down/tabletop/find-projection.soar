sp {op_put-down1*tabletop*propose*find-projection
   (state <s> ^name op_put-down1
              ^problem-space.execution-type external
             -^current-task-segment.projection-result
              ^held-object <held>
              ^destination <dest>
              ^predicate-handle <pred-handle>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name find-projection
        ^predicate-handle <pred-handle>
        ^arg-list <args>)
   (<args> ^1 <held> ^2 <dest>)
}

sp {op_put-down1*tabletop*apply*find-projection*copy*result
   (state <s> ^name op_put-down1
              ^operator.name find-projection
              ^projection-result <res>
              ^current-task-segment <seg>)
-->
