sp {op_put-down1*tabletop*apply*complete-task*remove*grabbed
   (state <s> ^name op_put-down1
              ^operator <o>
              ^held-object.predicates <preds>)
   (<o> ^name complete-task
        ^status success)
   (<preds> ^arm-status grabbed)
-->
   (<preds> ^arm-status grabbed -
            ^arm-status not-grabbed)
}

sp {op_put-down1*tabletop*apply*complete-task*remove*put-down-command
   (state <s> ^name op_put-down1
              ^operator <o>
              ^current-task-segment <a>
              ^io.output-link <out>)
   (<o> ^name complete-task
        ^status success)
   (<a> ^put-down-command <cmd>)
   (<out> ^put-down <cmd>)
-->
   (<a> ^put-down-command <cmd> -)
   (<out> ^put-down <cmd> -)
}
