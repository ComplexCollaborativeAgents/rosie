
sp {op_put-down1*magicbot*apply*complete-task*add*in*predicate
   (state <s> ^name op_put-down1
              ^operator <o>
              ^held-object <obj>
              ^top-state.world.predicates.predicate <in-pred>
              ^top-state.world.robot.current-location <loc>)
   (<o> ^name complete-task
        ^status success)
   (<in-pred> ^handle in1)
   -{(<in-pred> ^instance <cur-i>)
     (<cur-i> ^1 <obj> ^2 <loc>)}
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
}


