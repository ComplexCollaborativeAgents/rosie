sp {op_pick-up1*magicbot*apply*complete-task*remove*in*predicate
   (state <s> ^name op_pick-up1
              ^operator <o>
              ^target-object <obj>
              ^top-state.world.predicates.predicate <in-pred>
              ^top-state.world.robot.current-location <loc>)
   (<o> ^name complete-task
        ^status success)
   (<in-pred> ^handle in1
              ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
-->
   (<in-pred> ^instance <i> -)
}


sp {op_pick-up1*magicbot*apply*complete-task*remove*holding*predicate
   (state <s> ^name op_pick-up1
              ^operator <o>
              ^target-object <obj>
              ^top-state.world.predicates.predicate <in-pred>)
   (<o> ^name complete-task
        ^status success)
   (<in-pred> ^handle holding1
              ^instance <i>)
   (<i> ^1 <obj> ^2 <person>)
-->
   (<in-pred> ^instance <i> -)
}


