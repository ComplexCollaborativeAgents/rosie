sp {op_open1*internal*propose*perform-open
   (state <s> ^name op_open1
              ^agent-params.domain internal
              ^problem-space.execution-type external
              ^target-object.predicates.door2 closed2)
-->
   (<s> ^operator <o> +)
   (<o> ^name perform-open)
}

sp {op_open1*internal*apply*perform-open
   (state <s> ^name op_open1
              ^agent-params.domain internal
              ^problem-space.execution-type external
              ^operator.name perform-open
              ^object-info.input-link-obj.property <il-prop>)
   (<il-prop> ^property-handle door2
              ^values <vals>)
-->
   (<vals> ^closed2 1.00 -
           ^open2 1.00)
}

sp {op_open1*internal*prefer*attend-to-scene-change*over*perform-open
   (state <s> ^name op_open1
              ^agent-params.domain internal
              ^problem-space.execution-type external
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << push-task-segment pop-task-segment attend-to-scene-change >>)
   (<o2> ^name perform-open)
-->
   (<s> ^operator <o1> > <o2>)
}
