sp {execute*op_open1*propose*simulate-open-command
   (state <s> ^name op_open1
              ^problem-space.execution-type external
              ^agent-params.domain internal
              ^current-task-segment <seg>)
   (<seg> ^open-command <cmd>)
   (<cmd> ^name open
          ^object <obj>
         -^status complete)
-->
   (<s> ^operator <o> +)
   (<o> ^name simulate-open-command
        ^item-type command-operator
        ^command <cmd>)
}

sp {execute*op_open1*apply*simulate-open-command
   (state <s> ^name op_open1
              ^operator <o>
              ^top-state.perception-monitor.object-monitor.object-info <obj-info>)
   (<o> ^name simulate-open-command
        ^command <cmd>)
   (<cmd> ^object <obj>)
   (<obj-info> ^wm-obj <obj>
               ^input-link-obj.property <il-prop>)
   (<il-prop> ^property-handle door2
              ^values <vals>)
-->
   (<vals> ^closed2 1.00 -
           ^open2 1.00)
   (<cmd> ^status complete)
}

