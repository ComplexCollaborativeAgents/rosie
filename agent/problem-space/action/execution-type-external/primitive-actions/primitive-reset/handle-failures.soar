sp {primitive-reset*propose*handle-execution-failure
   (state <s> ^name primitive-reset
              ^current-task-segment <seg>)
   (<seg> ^reset-command <cmd>)
   (<cmd> ^status failure)
-->
   (<s> ^operator <o> + = >)
   (<o> ^name handle-execution-failure
        ^category failure-handling
        ^task-segment <seg>
        ^command <cmd>)
}

sp {primitive-reset*propose*handle-command-failure
   (state <s> ^name primitive-reset
              ^current-task-segment <seg>)
   (<seg> ^reset-command <cmd>)
   (<cmd> ^status error)
-->
   (<s> ^operator <o> + = >)
   (<o> ^name handle-command-failure
        ^category failure-handling
        ^task-segment <seg>
        ^command <cmd>)
}
