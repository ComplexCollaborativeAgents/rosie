sp {primitive-reset*external*elaborate*problem-space*attend-to-all-perception
   (state <s> ^name primitive-reset
              ^problem-space.execution-type external
              ^problem-space.perception <p>)
-->
   (<p> ^attend-to-all-perception yes)
}

sp {primitive-reset*elaborate*executing-command*true
   (state <s> ^name primitive-reset
              ^current-task-segment.reset-command.status sent)
-->
   (<s> ^executing-command true)
}

sp {primitive-reset*elaborate*propose-wait*true
   (state <s> ^name primitive-reset
              ^executing-command true)
-->
   (<s> ^propose-wait true)
}

sp {primitive-reset*elaborate*desired*satisfied
   (state <s> ^name primitive-reset
              ^problem-space.execution-type external
              ^current-task-segment.reset-command.status success)
-->
   (<s> ^desired.satisfied true)
}
