
sp {primitive-open*ai2thor*propose*send-open-command
   (state <s> ^name primitive-open
              ^agent-params.domain ai2thor
              ^problem-space.execution-type external
              -^current-task-segment.open-command
              ^target-object <obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name send-open-command
        ^object <obj>)
}

sp {primitive-open*ai2thor*apply*send-open-command*send
   (state <s> ^name primitive-open
              ^agent-params.domain ai2thor
              ^operator <o>
              ^io.output-link <ol>
              ^current-task-segment <seg>)
   (<o> ^name send-open-command
        ^object.handle <h>)
-->
   (<ol> ^perform-action <pa>)
   (<pa> ^name open
         ^object <h>)
   (<seg> ^open-command <pa>)
}
