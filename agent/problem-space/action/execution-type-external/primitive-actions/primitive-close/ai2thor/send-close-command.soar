
sp {primitive-close*ai2thor*propose*send-close-command
   (state <s> ^name primitive-close
              ^agent-params.domain ai2thor
              ^problem-space.execution-type external
              -^current-task-segment.close-command
              ^target-object <obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name send-close-command
        ^object <obj>)
}

sp {primitive-close*ai2thor*apply*send-close-command*send
   (state <s> ^name primitive-close
              ^agent-params.domain ai2thor
              ^operator <o>
              ^io.output-link <ol>
              ^current-task-segment <seg>)
   (<o> ^name send-close-command
        ^object.handle <h>)
-->
   (<ol> ^perform-action <pa>)
   (<pa> ^name close
         ^object <h>)
   (<seg> ^close-command <pa>)
}
