sp {op_close1*internal*propose*perform-close
   (state <s> ^name op_close1
              ^agent-params.domain internal
              ^problem-space.execution-type external
              ^target-object.predicates.door2 open2)
-->
   (<s> ^operator <o> +)
   (<o> ^name perform-close)
}

sp {op_close1*internal*apply*perform-close
   (state <s> ^name op_close1
              ^agent-params.domain internal
              ^problem-space.execution-type external
              ^operator.name perform-close
              ^object-info.input-link-obj.property <il-prop>)
   (<il-prop> ^property-handle door2
              ^values <vals>)
-->
   (<vals> ^open2 1.00 -
           ^closed2 1.00)
}

sp {op_close1*internal*prefer*attend-to-scene-change*over*perform-close
   (state <s> ^name op_close1
              ^agent-params.domain internal
              ^problem-space.execution-type external
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << push-task-segment pop-task-segment attend-to-scene-change >>)
   (<o2> ^name perform-close)
-->
   (<s> ^operator <o1> > <o2>)
}
