
sp {primitive-turn-on*ai2thor*propose*send-turn-on-command
   (state <s> ^name primitive-turn-on
              ^agent-params.domain ai2thor
              ^problem-space.execution-type external
              -^current-task-segment.turn-on-command
              ^target-object <obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name send-turn-on-command
        ^object <obj>)
}

sp {primitive-turn-on*ai2thor*apply*send-turn-on-command*send
   (state <s> ^name primitive-turn-on
              ^agent-params.domain ai2thor
              ^operator <o>
              ^io.output-link <ol>
              ^current-task-segment <seg>)
   (<o> ^name send-turn-on-command
        ^object <obj>)
   (<obj> ^handle <h>
          ^predicates.name microwave1)
-->
   (<ol> ^perform-action <pa>)
   (<pa> ^name set-timer
         ^object <h>
         ^time 15)
   (<seg> ^turn-on-command <pa>)
}
