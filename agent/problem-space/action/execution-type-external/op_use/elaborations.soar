sp {op_use1*elaborate*using-object*superoperator
   (state <s> ^name op_use1
              ^task-operator.arg1.id <obj>)
-->
   (<s> ^using-object <obj>)
}

sp {op_use1*elaborate*target-object*superoperator
   (state <s> ^name op_use1
              ^task-operator.arg2.{2} <obj2>)
-->
   (<s> ^target-object <obj2>)
}


sp {op_use1*elaborate*desired*satisfied*command*done
   (state <s> ^name op_use1
              ^agent-params.domain ai2thor
              ^current-task-segment.use-command.status success)
-->
   (<s> ^desired.satisfied true)
}
