sp {task-utils*copy-task-goal*propose*complete-copy-task-goal
   (state <s> ^copy-task-goal-workspace <ws>)
-->
   (<s> ^operator <o> + < =)
   (<o> ^name complete-copy-task-goal
        ^workspace <ws>)
}

sp {COPY*TASK*GOAL*learn-copy-task-goal-elaboration-rule
   (state <s> ^operator <o>
              ^superstate <ss>)
   (<o> ^name complete-copy-task-goal
        ^workspace <ws>)
   (<ws> ^source-goal <src>
         ^copy-predicate-set.arg-copy <copy>)
   (<src> ^handle <h>)
-->
   (<ss> ^desired <copy>)
   (<copy> ^duplicate-of <src>
           ^item-type task-goal
           ^handle <h>)
}

