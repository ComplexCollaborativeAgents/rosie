####################################################
# waypoint arguments

sp {task-utils*copy-task-operator*propose*copy-task-argument*waypoint
   (state <s> ^copy-task-op-workspace <ws>)
   (<ws> ^copy-argument <arg>)
   (<arg> ^arg-type waypoint
          ^source-arg.id <wp>
         -^arg-copy)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-task-argument
        ^workspace <ws>
        ^argument <arg>
        ^arg-type waypoint
        ^waypoint <wp>)
}

sp {task-utils*copy-task-operator*apply*copy-task-argument*waypoint
   (state <s> ^operator <o>)
   (<o> ^name copy-task-argument
        ^argument <arg>
        ^arg-type waypoint
        ^waypoint <wp>)
-->
   (<arg> ^arg-copy <copy>)
   (<copy> ^arg-type waypoint
           ^id <wp>)
}
