sp {instantiate-task-operator*elaborate*operator*info
   (state <s> ^name instantiate-task-operator
              ^superstate.operator <ssop>)
   (<ssop> ^name instantiate-task-operator
           ^destination <ito>
           ^general-task-rep <rep>     
           ^reference-task-op <task-op>   
           ^reference-tcn-proc <tcn>
           ^world <w>)
-->
   (<s> ^result-destination <ito>
        ^general-task-rep <rep>     
        ^reference-task-op <task-op>   
        ^reference-tcn-proc <tcn>
        ^world <w>)
}

sp {instantiate-task-operator*elaborate*perform*from*general-task-rep
   (state <s> ^name instantiate-task-operator
              ^general-task-rep.perform <perf>)
-->
   (<s> ^perform <perf>)
}

sp {instantiate-task-operator*elaborate*perform*default
   (state <s> ^name instantiate-task-operator
              ^general-task-rep <src-id>)
   (<src-id> -^perform)
-->
   (<s> ^perform multiple)
}

# elaborate argument information 
## ENUMERATE: 0-5 arguments
sp {instantiate-task-operator*elaborate*no-args
   (state <s> ^name instantiate-task-operator
              ^general-task-rep <rep>)
   (<rep> -^{ << arg1 arg2 arg3 arg4 arg5 until-clause >> } <arg>)
-->
   (<s> ^num-args 0)
}

sp {instantiate-task-operator*elaborate*arguments*1*arg
   (state <s> ^name instantiate-task-operator
              ^general-task-rep <rep>)
   (<rep> ^{ <n1>        << arg1 arg2 arg3 arg4 arg5 until-clause >> } <a1>
         -^{ <n2> <> <n1> << arg1 arg2 arg3 arg4 arg5 until-clause >> } <a2>)
   (<a1> ^arg-type <t1>)
-->
   (<s> ^num-args 1
        ^instantiate-task-argument <ita1>)
   (<ita1> ^arg-type <t1>
          ^arg-name <n1>
          ^general-arg-rep <a1>)
}

