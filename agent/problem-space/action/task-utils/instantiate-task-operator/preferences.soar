#####                preferences                       ########

sp {instantiate-task-operator*prefer*instantiate-task-argument*procedural*over*others
  (state <s> ^name instantiate-task-operator
             ^operator <o1> +
             ^operator <o2> +)
  (<o1> ^name instantiate-task-argument
        ^argument <arg>
        ^match-type procedural)
  (<o2> ^name instantiate-task-argument
        ^argument <arg>
        ^match-type << default abstract >>)
-->
  (<s> ^operator <o1> > <o2>)
}

sp {instantiate-task-operator*prefer*instantiate-task-argument*default*over*others
  (state <s> ^name instantiate-task-operator
             ^operator <o1> +
             ^operator <o2> +)
  (<o1> ^name instantiate-task-argument
        ^argument <arg>
        ^match-type default)
  (<o2> ^name instantiate-task-argument
        ^argument <arg>
        ^match-type abstract)
-->
  (<s> ^operator <o1> > <o2>)
}

sp {instantiate-task-operator*prefer*instantiate-task-argument*over*complete-instantiate-task-operator
  (state <s> ^name instantiate-task-operator
             ^operator <o1> +
             ^operator <o2> +)
  (<o1> ^name instantiate-task-argument)
  (<o2> ^name complete-instantiate-task-operator)
-->
  (<s> ^operator <o1> > <o2>)
}
