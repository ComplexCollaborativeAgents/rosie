sp {generalize-task-operator*elaborate*operator*info
   (state <s> ^name generalize-task-operator
              ^superstate.operator <ss-op>)
   (<ss-op> ^name generalize-task-operator
            ^destination <dest>
            ^task-operator <task-op>        
            ^task-concept-network <tcn>
            ^reference-task-op <ref-task>   
            ^reference-tcn-proc <ref-proc>)
-->
   (<s> ^result-destination <dest>
        ^task-operator <task-op>
        ^task-concept-network <tcn>
        ^reference-task-op <ref-task>
        ^reference-tcn-proc <ref-proc>)
}

# elaborate argument information 
## ENUMERATE: 0-5 arguments
sp {generalize-task-operator*elaborate*no-args
   (state <s> ^name generalize-task-operator
              ^task-operator <sto>)
   (<sto> -^{ << arg1 arg2 arg3 arg4 arg5 until-clause after-clause >> } <arg>)
-->
   (<s> ^num-args 0)
}

sp {generalize-task-operator*elaborate*arguments*1*arg
   (state <s> ^name generalize-task-operator
              ^task-operator <sto>)
   (<sto> ^{ <n1>        << arg1 arg2 arg3 arg4 arg5 until-clause after-clause >> } <a1>
         -^{ <n2> <> <n1> << arg1 arg2 arg3 arg4 arg5 until-clause after-clause >> } <a2>)
   (<a1> ^arg-type <t1>)
-->
   (<s> ^num-args 1
        ^generalize-task-argument <ga1>)
   (<ga1> ^arg-type <t1>
          ^arg-name <n1>
          ^instantiation <a1>)
}

