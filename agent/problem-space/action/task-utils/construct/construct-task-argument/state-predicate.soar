### construct state-predicate

sp {task-utils*construct-task-argument*expand*state-predicate
   (state <s> ^construct-task-argument <arg>)
   (<arg> ^arg-type predicate
          ^source <src>)
   (<src> ^type state
          ^1 <obj1>)
-->
   (<arg> ^pred-type state)

   (<s> ^construct-task-argument <obj1-arg>)
   (<obj1-arg> ^arg-type object
               ^source <obj1>)
   (<arg> ^create-obj1 <obj1-arg>)
}
          

sp {task-utils*construct-task-argument*propose*construct-task-argument*state-predicate
   (state <s> ^construct-task-argument <arg>)
   (<arg> ^pred-type state
          ^source.handle <handle>
          ^create-obj1.created-arg.id <obj1>
