sp {task-utils*copy-desired*propose*copy-desired-predicate*state
   (state <s> ^copy-desired-workspace <ws>)
   (<ws> ^copy-predicate <pred>
         ^object-map <map>)
   (<pred> ^source-pred <src-pred>
          -^pred-copy)
   (<src-pred> ^type state
               ^handle <pred-handle>
               ^property-handle <prop-handle>
               ^1 <src-obj>)
   (<map> ^<src-obj> <copy-obj>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name copy-desired-predicate
        ^workspace <ws>
        ^predicate <pred>
        ^type state
        ^pred-handle <pred-handle>
        ^prop-handle <prop-handle>
        ^1 <copy-obj>)
}

sp {task-utils*copy-desired*apply*copy-desired-predicate*state
   (state <s> ^operator <o>)
   (<o> ^name copy-desired-predicate
        ^workspace <ws>
        ^predicate <pred>
        ^type state
        ^pred-handle <pred-handle>
        ^prop-handle <prop-handle>
        ^1 <obj>)
-->
   (<pred> ^pred-copy <copy>)
   (<copy> ^item-type goal-predicate 
           ^type state
           ^handle <pred-handle>
           ^property-handle <prop-handle>
           ^1 <obj>)
}
