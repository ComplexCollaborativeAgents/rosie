# remove new-task-segment from task-stack
sp {task-utils*push-task-segment*apply*complete-push-task-segment*remove*new-task-segment*task-stack
   (state <s> ^name push-task-segment
              ^operator.name complete-push-task-segment
              ^segment-to-push <seg>
              ^task-stack <stack>)
   (<stack> ^new-task-segment <seg>)
-->
   (<stack> ^new-task-segment <seg> -)
}

# remove new-task-segment from superstate
sp {task-utils*push-task-segment*apply*complete-push-task-segmennt*remove*new-task-segmennt*superstate
   (state <s> ^name push-task-segment
              ^operator.name complete-push-task-segment
              ^segment-to-push <seg>
              ^superstate <ss>)
   (<ss> ^problem-space.name action
         ^new-task-segment <seg>)
-->
   (<ss> ^new-task-segment <seg> -)
}

# Mark status complete on superoperator
sp {task-utils*push-task-segment*apply*complete-push-task-segment*mark*status*complete*on*superoperator
   (state <s> ^name push-task-segment
              ^operator.name complete-push-task-segment
              ^superstate.operator <o>)
-->
   (<o> ^status complete)
}

# push the task on to the stack

sp {task-utils*push-task-segment*apply*complete-push-task-segment*push
   (state <s> ^name push-task-segment
              ^operator.name complete-push-task-segment
              ^task-stack <stack>
              ^segment-to-push <seg>)
   (<stack> ^top <top>)
   (<top> ^next none)
   (<seg> ^task-operator <task-op>)
-->
   (<stack> ^top <top> -
            ^top <seg>)
   (<seg> ^next none
          ^prev <top>
          ^start-of-execution true)
   (<top> ^next none -
          ^next <seg>)
   (<task-op> ^current-goal first)
}

# if the stack was empty, update the top and bottom 
sp {task-utils*push-task-segment*apply*complete-push-task-segment*push*empty
   (state <s> ^name push-task-segment
              ^operator.name complete-push-task-segment
              ^task-stack <stack>
              ^segment-to-push <seg>)
   (<stack> ^top none
            ^bottom none)
   (<seg> ^task-operator <task-op>)
-->
   (<stack> ^top none -
            ^top <seg>
            ^bottom none -
            ^bottom <seg>)
   (<seg> ^next none
          ^prev none
          ^start-of-execution true)
   (<task-op> ^current-goal first)
}

