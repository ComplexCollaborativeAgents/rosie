# If no known proposal rule matched, then just copy the created one to the superstate

sp {learn-subaction-proposal*state*no-change*elaborate*name*learn-subaction-proposal-substate
   (state <s> ^impasse no-change
              ^attribute state
              ^superstate.name learn-subaction-proposal
             -^learning-task-utils-rule true)
-->
   (<s> ^name learn-subaction-proposal-substate)
}

sp {learn-subaction-proposal*state*no-change*propose*report-no-subaction-matches
   (state <s> ^name learn-subaction-proposal-substate)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-no-subaction-matches)
}

# CREATE RESULT
sp {learn-subaction-proposal*state*no-change*apply*report-no-subaction-matches
   (state <s> ^name learn-subaction-proposal-substate
              ^operator.name report-no-subaction-matches
              ^quiescence t
              ^superstate <ss>)
-->
   (<ss> ^no-subaction-matches true)
}

