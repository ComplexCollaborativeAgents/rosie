sp {learn-subaction-proposal*elaborate*default-problem-space*no
   (state <s> ^name learn-subaction-proposal)
-->
   (<s> ^default-problem-space no)

   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^execution-type simulate
         ^world.copy-type shallow)
}

sp {learn-subaction-proposal*elaborate*problem-space*propose-action-operators
   (state <s> ^name learn-subaction-proposal
              ^subaction-operator <sub>
              ^problem-space <ps>)
-->
   (<ps> ^propose-action-operators true)
}
