sp {learn-subaction-proposal*propose*create-operator
   (state <s> ^name learn-subaction-proposal
              ^instantiate-task-operator <ito>
              -^subaction-operator)
   (<ito> ^instantiation <tkop>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name create-operator
        ^subtask-operator <tkop>)
}

sp {learn-subaction-proposal*apply*create-operator*copy*subtask-handle
   (state <s> ^name learn-subaction-proposal
              ^subaction-tcn.handle <h>
              ^operator <o>)
   (<o> ^name create-operator
        ^subtask-operator <tkop>)
-->
   (<tkop> ^subtask-handle <h>)
}

sp {learn-subaction-proposal*apply*create-operator*copy*subaction-operator
   (state <s> ^name learn-subaction-proposal
              ^operator <o>)
   (<o> ^name create-operator
        ^subtask-operator <a>)
-->
   (<s> ^subaction-operator <a>)
}
