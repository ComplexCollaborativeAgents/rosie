sp {action*learn-desired-elaboration-rule*reject*instantiate-task-argument*abstract
  (state <s> ^name learn-desired-elaboration-rule
             ^operator <o> +)
  (<o> ^name instantiate-task-argument
       ^match-type abstract)
-->
  (<s> ^operator <o> -)
}

sp {action-learn-desired-elaboration-rule*prefer*complete*over*return-empty
  (state <s> ^name learn-desired-elaboration-rule
              ^operator <o1> +
              ^operator <o2> +)
  (<o1> ^name complete-learn-desired-elaboration-rule)
  (<o2> ^name create-empty-goal)
-->
  (<s> ^operator <o1> > <o2>)
}

sp {action*learn-desired-elaboration-rule*reject*complete-instantiate-task-operator
   (state <s> ^name learn-desired-elaboration-rule
              ^operator <o> +)
   (<o> ^name complete-instantiate-task-operator)
-->
   (<s> ^operator <o> -)
}
