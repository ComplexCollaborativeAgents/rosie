sp {learn-desired-elaboration-rule*propose*create-emptpy-goal
  (state <s> ^name learn-desired-elaboration-rule)
-->
  (<s> ^operator <o> + <)
  (<o> ^name create-empty-goal)
}

sp {learn-desired-elaboration-rule*elaborate*unsatisfied-conditions*true
  (state <s> ^name learn-desired-elaboration-rule
             ^predicate-test <t>
             ^instantiate-task-argument <arg>)
   (<arg> ^arg-type predicate-set
          ^type goal-conditions
          ^instantiation <t>)
   (<t> -^satisfied true)
-->
   (<s> ^unsatisfied-conditions true)
}

sp {learn-desired-elaboration-rule*apply*create-emptpy-goal
  (state <s> ^name learn-desired-elaboration-rule
             ^quiescence t
             ^operator.name create-empty-goal
             ^superstate.superstate <ss>
            -^unsatisfied-conditions true)
-->
  (<ss> ^desired <des>)
}

sp {learn-desired-elaboration-rule*apply*create-empty-goal*unsatisfied-conditions
  (state <s> ^name learn-desired-elaboration-rule
             ^quiescence t
             ^operator.name create-empty-goal
             ^superstate.superstate <ss>
             ^goal-id.handle <h>
             ^unsatisfied-conditions true)
-->
  (<ss> ^desired <des>)
  (<des> ^handle <h>
         ^unsatisfied-conditions true)
}
