
sp {handle-missing-desired*elaborate*superstate*info
   (state <s> ^name handle-missing-desired
              ^superstate <ss>)
   (<ss> ^task-operator <task-op>
         ^task-concept-network <tcn>
         ^world <w>)
-->
   (<s> ^task-operator <task-op>
        ^task-concept-network <tcn>
        ^world <w>)
}

sp {handle-missing-desired*elaborate*no-current-goal
   (state <s> ^name handle-missing-desired
              ^task-operator.current-goal << first none >>)
-->
   (<s> ^no-current-goal true)
}
