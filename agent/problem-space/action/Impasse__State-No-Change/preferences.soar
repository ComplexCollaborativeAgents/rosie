# Priority: 
# 0: smem-query (retrieve-tcn)
# 1: handle-missing-desired
# 2: learn-subtask-proposal
# 3: search-for-next-subtask

sp {task-state-no-change*prefer*handle-missing-desired*over*others
   (state <s> ^name task-state-no-change
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name handle-missing-desired)
   (<o2> ^name << learn-subtask-proposal search-for-next-subtask >>)
-->
   (<s> ^operator <o1> > <o2>)
}

