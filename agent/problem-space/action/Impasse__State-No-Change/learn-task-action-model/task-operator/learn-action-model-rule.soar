sp {__TASK__ACTION__MODEL__*learn-task-action-model*1*relation
   (state <s> ^name learn-task-action-model-substate
              ^task-operator <task-op>
              ^superstate <ss>
              ^desired <des>)
   (<ss> ^operator <task-op>
         ^superstate <sss>
         ^problem-space <ps>
         ^world.predicates.predicate <pred>)
   (<ps> ^name action
         ^execution-type internal)
   (<des> ^pred-count 1
          ^1 <p1>)
   (<p1> ^type relation
         ^handle <ph>
         ^1 <obj1>
         ^2 <obj2>)
   (<pred> ^handle <ph>)
   -{(<pred> ^instance <i>)
     (<i> ^1 <obj1> ^2 <obj2>)}
-->
    (<pred> ^instance <i>)
    (<i> ^1 <obj1> ^2 <obj2>)
}

sp {learn-task-action-model-substate*report*learning-finished*superstate
   (state <s> ^name learn-task-action-model-substate
              ^task-operator <task-op>
              ^superstate <ss>
              ^desired <des>
              ^quiescence t)
-->
   (<ss> ^learning-finished true)
}

