sp {task-state-no-change*propose*search-for-next-subtask
   (state <s> ^name task-state-no-change
             -^learning-task-utils-rule true
              ^desired <d>)

-->
   (<s> ^operator <o> + =)
   (<o> ^name search-for-next-subtask)
}

