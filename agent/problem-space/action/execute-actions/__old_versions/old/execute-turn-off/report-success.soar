sp {op_turn-off*propose*report-success
   (state <s> ^name op_turn-off1
              ^target-object.predicates.activation1 off2)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {op_turn-off*apply*report-success*add*status
   (state <s> ^name op_turn-off1
              ^operator.name report-success
              ^current-action <a>)
-->
   (<a> ^status complete)
}

sp {op_turn-off*apply*report-success*remove*command
   (state <s> ^name op_turn-off1
              ^operator.name report-success
              ^current-action <a>
              ^io.output-link <out>)
   (<a> ^turn-off-command <cmd>)
   (<out> ^<cmd-name> <cmd>)
-->
   (<a> ^turn-off-command <cmd> -)
   (<out> ^<cmd-name> <cmd> -)
}
