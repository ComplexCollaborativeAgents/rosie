sp {op_close*propose*send-close-command
   (state <s> ^name op_close1
              ^target-object <obj>
             -^current-action.close-command <cmd>)
-->
   (<s> ^operator <o> +)
   (<o> ^name send-close-command
        ^object <obj>)
}

sp {op_close*apply*send-close-command
   (state <s> ^name op_close1
              ^operator <o>
              ^top-state.io.output-link <out>
              ^current-action <a>)
   (<o> ^name send-close-command
        ^object.handle <obj-handle>)
-->
   (<out> ^set-state <set>)
   (<set> ^object-handle <obj-handle>
          ^name door1
          ^value closed2)
   (<a> ^close-command <set>)
}
   
              
              
