sp {op_give1*propose*report-success*in*destination
   (state <s> ^name op_give1
              ^current-action.put-down-command.status success)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {op_give*apply*report-success*add*holding*predicate
   (state <s> ^name op_give1
              ^operator.name report-success
              ^held-object <obj>
              ^person <person>
              ^top-state.world.predicates.predicate <in-pred>)
   (<in-pred> ^handle holding1)
   -{(<in-pred> ^instance <cur-i>)
     (<cur-i> ^1 <obj> ^2 <person>)}
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <person> ^2 <obj>)
}

sp {op_give*apply*report-success*add*in*predicate
   (state <s> ^name op_give1
              ^operator.name report-success
              ^held-object <obj>
              ^top-state.world.predicates.predicate <in-pred>
              ^top-state.world.robot.current-location <loc>)
   (<in-pred> ^handle in1)
   -{(<in-pred> ^instance <cur-i>)
     (<cur-i> ^1 <obj> ^2 <loc>)}
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
}
