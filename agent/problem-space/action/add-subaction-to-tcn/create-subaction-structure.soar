sp {add-subaction-to-tcn*propose*create-subaction-structure
   (state <s> ^name add-subaction-to-tcn
              ^generalize-task-operator.general-task-rep <gen-task>
             -^subaction-structure)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-subaction-structure
        ^general-subtask-rep <gen-task>)
}

sp {add-subaction-to-tcn*apply*create-subaction-structure*create*subaction*handle
   (state <s> ^name add-subaction-to-tcn
              ^operator <o>)
   (<o> ^name create-subaction-structure
        ^general-subtask-rep <gen-sub>)
-->
   (<gen-sub> ^handle (make-constant-symbol |subaction|))
}

sp {add-subaction-to-tcn*apply*create-subaction-structure*mark*learned*no
   (state <s> ^name add-subaction-to-tcn
              ^operator <o>)
   (<o> ^name create-subaction-structure
        ^general-subtask-rep <gen-sub>)
-->
   (<gen-sub> ^learned no)
}

sp {add-subaction-to-tcn*apply*create-subaction-structure*copy*general-subtask-rep*as*subaction-structure
   (state <s> ^name add-subaction-to-tcn
              ^operator <o>)
   (<o> ^name create-subaction-structure
        ^general-subtask-rep <gen-sub>)
-->
   (<s> ^subaction-structure <gen-sub>)
}
