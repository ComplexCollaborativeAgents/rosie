sp {add-subaction-to-tcn*propose*create-subaction-structure
   (state <s> ^name add-subaction-to-tcn
              ^generalize-action.generalized-action <gen-action>
             -^subaction-structure)
-->
   (<s> ^operator <o> +)
   (<o> ^name create-subaction-structure
        ^generalized-action <gen-action>)
}

sp {add-subaction-to-tcn*apply*create-subaction-structure*create*subaction*handle
   (state <s> ^name add-subaction-to-tcn
              ^operator <o>)
   (<o> ^name create-subaction-structure
        ^generalized-action <gen-action>)
-->
   (<gen-action> ^handle (make-constant-symbol |subaction|))
}

sp {add-subaction-to-tcn*apply*create-subaction-structure*mark*learned*no
   (state <s> ^name add-subaction-to-tcn
              ^operator <o>)
   (<o> ^name create-subaction-structure
        ^generalized-action <gen-action>)
-->
   (<gen-action> ^learned no)
}

sp {add-subaction-to-tcn*apply*create-subaction-structure*copy*generalized-action*as*subaction-structure
   (state <s> ^name add-subaction-to-tcn
              ^operator <o>)
   (<o> ^name create-subaction-structure
        ^generalized-action <gen-action>)
-->
   (<s> ^subaction-structure <gen-action>)
}
