smem --add {
                             
           (@6010000 ^structure-type CN
                   ^assigners <CN1> <CN2> <CN3>
                   ^converts-to DP
                   ^specifier indefinite)
           (<CN1> ^structure-type ADJ
                   ^syntactic-structure adjoin
                   ^repeatable true
                   ^relative-position after
                   ^optional true
                   ^before <CN2>)
           (<CN2> ^structure-type N
                   ^syntactic-structure head 
                   ^relative-position after
                   ^optional true
                   ^before <CN3>)
           (<CN3> ^structure-type PP
                   ^syntactic-structure complement
                   ^relative-position after
                   ^optional true)

           (<middle> ^spelling |middle-noun|
                    ^prior-word D
                    ^current-word |middle|
                    ^structure-type N
                    ^consumes-prior-word false
                    ^semantic-entity-type relation
                    ^semantic-structure predicate
                    ^number singular
                    ^relation <middle-of1>)
           (<middle-of1> ^handle middle-of1)
                                
           (<front> ^spelling |front-noun|
                    ^prior-word P
                    ^current-word |front|
                    ^structure-type DP
                    ^consumes-prior-word false
                    ^semantic-entity-type relation
                    ^semantic-structure predicate
                    ^number singular
                    ^relation <front-of1>)
           (<front-of1> ^handle front-of1)
                                    
           (<back> ^spelling |back-noun|
                    ^prior-word P
                    ^current-word |back|
                    ^structure-type DP
                    ^consumes-prior-word false
                    ^semantic-entity-type relation
                    ^semantic-structure predicate
                    ^number singular
                    ^relation <back-of1>)      
           (<back-of1> ^handle back-of1)            


