           (<turned> ^spelling |turned|
                     ^structure-type V
                     ^tense past
                     ^can-be-part-of |turned-on|)     
           (<off2> ^prior-word |turned|
                   ^current-word |off|
                   ^spelling |turned-off|
                   ^structure-type ADJ
                   ^referent @4014002
                   ^attachment immediate
                   ^consumes-prior-word true)
           (<off3>  ^spelling |off|
                    ^structure-type ADJ
                    ^attachment immediate
                   #^alternative-structure P
                    ^referent @4014002)

           (<on1> ^prior-word |turned|
                  ^current-word |on|
                  ^spelling |turned-on|
                  ^structure-type ADJ
                  ^attachment immediate
                  ^referent @4014001
                  ^consumes-prior-word true)
           (<lit> ^spelling |lit|  # JEL 9/23/2015 - Avoid confusion with preposition right now
                  ^structure-type ADJ
                   ^attachment immediate
                  ^referent @4014001)  


           (<minutes> ^spelling |minutes|
                   ^structure-type N
                   ^referent <minute-id>
                   ^number plural)
           (<minute> ^spelling |minute|
                   ^structure-type N
                   ^number singular
                   ^referent <minute-id>)
           (<minute-id> ^handle minute ^property @4010000)

           (<answer> ^spelling |answer|
                     ^structure-type N
                     #           ^syntactic-referent-type N   #  Hmm - lots of possibilities here - |yes| |no| |dog|
                     ^number singular                     
                     #^object-feature handle
                     ^referent @4015003)
                     
           (<question> ^spelling |question|
                     ^structure-type N
#                     ^syntactic-referent-type N   #  Hmm - lots of possibilities here - |yes| |no| |dog|
                     ^number singular                     
#                     ^object-feature handle
                     ^referent @407015)
                      
           (<response> ^spelling |response|
                     ^structure-type N
                     ^syntactic-referent-type N   #  Hmm - lots of possibilities here - |yes| |no| |dog|
                     ^number singular                     
                     ^object-feature handle
                     ^referent <rex>)
           (<rex> ^handle response 
                  ^property <concept>) 
          
           (<message> ^spelling |message|
                     ^structure-type N
#                     ^syntactic-referent-type N   #  Hmm - lots of possibilities here - |yes| |no| |dog|
                     ^number singular                     
#                     ^object-feature handle
                     ^referent @407012)

           
           (<rank> ^spelling |rank|
                     ^structure-type N   
                     ^number singular      
                     ^referent <rankx>)
           (<rankx> ^handle rank1
                    ^property <concept>)   
       
           (<suit> ^spelling |suit|
                     ^structure-type N   
                     ^number singular      
                     ^referent <suitx>)
           (<suitx> ^handle suit 
                     ^property <concept>)
           
          (<doors>  ^spelling |doors|
                    ^structure-type N
                    ^number plural
                    ^referent @4012014)
           
           (<doorways>  ^spelling |doorways|
                    ^structure-type N
                    ^number plural
                    ^referent @4012014)

           (<bedroom1> ^spelling |bedroom|
