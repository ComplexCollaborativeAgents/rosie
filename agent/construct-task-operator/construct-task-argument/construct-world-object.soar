
# If the object is an smem object, create a world reprensetation for it
sp {construct-task-argument*propose*construct-world-object*from*smem
   (state <s> ^name construct-task-argument
              ^create-object-rep <cor>)
   (<cor> ^source <src-obj>
         -^object-rep)
   (<src-obj> ^object-instance true)
-->
   (<s> ^operator <o> + =)
   (<o> ^name construct-world-object
        ^source-type smem-obj
        ^source-obj <src-obj>
        ^destination <cor>
        ^result-name object-rep)
}

