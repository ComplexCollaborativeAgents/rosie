
sp {construct-task-operator*propose*push-task-segment
   (state <s> ^name construct-task-operator
              ^new-task-segment <seg>
             -^pushed <seg>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name push-task-segment
        ^segment <seg>)
}

sp {construct-task-operator*apply*push-task-segment*mark*pushed
   (state <s> ^name construct-task-operator
              ^operator <o>
              ^new-task-segment <seg>)
   (<o> ^name push-task-segment 
        ^segment <seg>
        ^status complete)
-->
   (<s> ^pushed <seg>)
}

