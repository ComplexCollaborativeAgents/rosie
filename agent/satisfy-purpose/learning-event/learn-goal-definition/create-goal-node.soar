sp {learn-goal-definition*propose*create-goal-node
   (state <s> ^name learn-goal-definition
             -^goal-node)
-->
   (<s> ^operator <o> + >)
   (<o> ^name create-goal-node)
}

sp {learn-goal-definition*apply*create-goal-node
   (state <s> ^name learn-goal-definition
              ^operator <o>)
   (<o> ^name create-goal-node)
-->
   (<s> ^goal-node <goal>)
   (<goal> ^item-type task-goal
           ^handle (make-constant-symbol |taskgoal|))
}
