sp {learn-whenever-task*propose*store-tcn
   (state <s> ^name learn-whenever-task
              ^task-concept-network <tcn>
              ^to-store <tcn>)
-->
   (<s> ^operator <o> +)
   (<o> ^name store-tcn)
}

sp {learn-whenever-task*apply*store-tcn*store*to-store
   (state <s> ^name learn-whenever-task
              ^operator.name store-tcn
              ^to-store <lti>
              ^smem.command <cmd>)
-->
   (<cmd> ^store <lti>)
   (<s> ^to-store <lti> -
        ^stored <lti>)
}
