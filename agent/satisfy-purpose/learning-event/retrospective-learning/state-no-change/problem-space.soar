sp {retrospective-learning*state-no-change*elaborate*name*learn-proposal-rule
   (state <s> ^superstate.name retrospective-learning
              ^impasse no-change
              ^attribute state)
-->
   (<s> ^name learn-proposal-rule)
}

sp {learn-proposal-rule*elaborate*problem-space
   (state <s> ^name learn-proposal-rule)
-->
   (<s> ^default-problem-space no)
   
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-space retrospective
         ^action <action>
         ^world <world>
         ^learning <learn>
         ^subactions <subs>)
   (<action> ^type simulate)
   (<world> ^copy-type shallow)
   (<learn> ^type retrospective)
   (<subs> ^copy-type shallow)
   #(<learn> ^use chunking) # AM: For now, not learning proposal rule
}
