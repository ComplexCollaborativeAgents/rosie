
sp {replay-subaction*elaborate*problem-space
   (state <s> ^name replay-subaction)
-->
   (<s> ^default-problem-space no)
   
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action-space retrospective
         ^action <action>
         ^world <world>
         ^learning <learn>
         ^subactions <subs>)
   (<action> ^type simulate)
   (<world> ^copy-type shallow)
   (<learn> ^type retrospective)
   (<subs> ^copy-type shallow)
}
