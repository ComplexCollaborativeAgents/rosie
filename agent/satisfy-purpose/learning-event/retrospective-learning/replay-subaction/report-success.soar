sp {replay-subaction*propose*report-success
   (state <s> ^name replay-subaction
              ^desired <des>)
   (<des> ^satisfied true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name report-success)
}

sp {replay-subaction*apply*report-success
   (state <s> ^name replay-subaction
              ^operator.name report-success
              ^subaction-context <sub>)
-->
   (<sub> ^learned true)
}
             
sp {replay-subaction*prefer*report-success*over*all*others
   (state <s> ^name replay-subaction
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name report-success)
   (<o2> ^name <> report-success)
-->
   (<s> ^operator <o1> > <o2>)
}
