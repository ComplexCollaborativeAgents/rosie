
sp {learn-proposal-rule*elaborate*problem-space
   (state <s> ^name learn-proposal-rule)
-->
   (<s> ^default-problem-space no)
   
   (<s> ^problem-space <ps>)
   (<ps> ^name action
         ^action <action>
         ^world <world>
         ^learning <learn>)
   (<action> ^type simulate)
   (<learn> ^type retrospective
            ^use chunking)
}

sp {learn-proposal-rule*elaborate*problem-space*world
   (state <s> ^name learn-proposal-rule
              ^problem-space.world <world>
              ^superstate.world <ssw>)
-->
   (<world> ^source <ssw>
            ^copy-type deep)
}
