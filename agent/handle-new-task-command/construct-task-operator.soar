sp {handle-new-task-command*propose*construct-task-operator
   (state <s> ^name handle-new-task-command
              ^command-message <msg>
              ^task-concept-network <tcn>
             -^task-operator)
-->
   (<s> ^operator <o> +)
   (<o> ^name construct-task-operator
        ^command-message <msg>
        ^task-concept-network <tcn>)
}

sp {handle-new-task-command*elaborate*construct-task-operator*conditions
   (state <s> ^name handle-new-task-command
              ^operator <o> +
              ^conditions <conds>)
   (<o> ^name construct-task-operator
        ^command-message <msg>)
-->
   (<msg> ^conditions <conds>)
}


