sp {top-state*propose*execute-task
   (state <s> ^superstate nil
              ^interaction.stack.segment.purpose.type << finished-game-learning execute-task >>
              ^task-stack.bottom { <seg> <> none })
-->
   (<s> ^operator <o> +)
   (<o> ^name execute-task
        ^task-segment <seg>)
}

sp {top-state*apply*execute-task
   (state <s> ^superstate nil
              ^operator.name execute-task
              ^agent-params.debug-tasks true
              ^agent-params.enable-interrupts true)
-->
   (interrupt)
}


### Will take a command message and create a new task segment with it
sp {top-state*propose*handle-new-task-command
   (state <s> ^superstate nil
              ^interaction.stack.segment.purpose <p>)
   (<p> ^type execute-task
        ^parameters <params>)
   (<params> ^command-message <msg>)
-->
   (<s> ^operator <o> +)
   (<o> ^name handle-new-task-command
        ^command-message <msg>)
}

